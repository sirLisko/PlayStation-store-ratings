{
  "name": "PS Store Ratings",
  "version": "0.1.0",
  "description": "Adds IGN rating to the games in the PS store.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://store.playstation.com/*"
      ],
      "css": ["style.css"],
      "js": ["base.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
